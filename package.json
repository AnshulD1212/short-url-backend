{
  "name": "node-backend",
  "version": "1.0.0",
  "description": "backend for short url service",
  "main": "dist/index.js",
  "repository": "git@github.com:AnshulD1212/short-url-backend.git",
  "author": "anshuld1212 <anshul.dharmadhikari@live.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev src/index.ts | pino-pretty",
    "build": "./node_modules/.bin/tsc",
    "start": "ts-node dist/index.js",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --collectCoverage=false --detectOpenHandles --unhandled-rejections=strict",
    "test:coverage": "yarn test --coverage",
    "plop": "plop",
    "migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -d ./src/dataSource.ts",
    "migration:up": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d ./src/dataSource.ts",
    "migration:down": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert -d ./src/dataSource.ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.5.0",
    "pino-pretty": "^10.0.0",
    "plop": "^3.1.2",
    "prettier": "^2.8.4",
    "redis-mock": "^0.56.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.2"
  },
  "dependencies": {
    "@types/cors": "^2.8.13",
    "@types/pg": "^8.6.6",
    "@types/redis": "^4.0.11",
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.10.0",
    "pino": "^8.11.0",
    "redis": "^4.6.6",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.12",
    "typeorm-naming-strategies": "^4.1.0"
  }
}
